[user]
name = tothegump
email = tothegump@gmail.com

[push]
    default = simple
[core]
    editor = vim
    excludesfile = ~/.gitignore_global

[alias]
    b = branch
    ci = commit
    cia = commit --amend
    cempty = commit --allow-empty -m
    co = checkout
    cob = checkout -b
    d = diff
    g = grep -in
    l = log
    st = status
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    lga = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --branches
	#############
	cp = cherry-pick
	cpa = cherry-pick --abort
	cpc = cherry-pick --continue
	#############
	f = fetch
	fo = fetch origin
	fu = fetch upstream
	#############
	m = merge
	ma = merge --abort
	mc = merge --continue
	ms = merge --skip
	#############
	ps = push
	psf = push -f
	psu = push -u
	pso = push origin
	psz = push zjmgitlab
	psao = push --all origin
	psfo = push -f origin
	psuo = push -u origin
	psom = push origin master
	#############
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbs = rebase --skip
	#############
	sa = stash apply
	sc = stash clear
	sd = stash drop
	sl = stash list
	sp = stash pop
	ss = stash save
	sw = stash show
    ############
    rst = reset


